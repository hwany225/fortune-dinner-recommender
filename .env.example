# Fortune Dinner Recommender 환경 설정 예시

# 서버 포트 설정
PORT=8001

# 개발 모드 설정
FLASK_ENV=development
FLASK_DEBUG=true

# CORS 설정 (개발 환경)
CORS_ORIGINS=*

# 보안 설정 (프로덕션에서 변경 필요)
SECRET_KEY=your-secret-key-here

# 로그 레벨
LOG_LEVEL=INFO

# 네트워크 접근 허용 여부
ALLOW_NETWORK_ACCESS=true

# 최대 동시 연결 수
MAX_CONNECTIONS=100

# API 요청 제한 (분당)
RATE_LIMIT_PER_MINUTE=60

# 캐시 설정
CACHE_TIMEOUT=3600

# 프론트엔드 경로 (상대 경로)
FRONTEND_PATH=../frontend